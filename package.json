{
	"name": "mujer-automotriz",
	"author": "Mujer Automotriz <hola@mujerautomotriz.com>",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"1password": "op run --no-masking --env-file .env.op --",
		"dev": "NODE_ENV=development bun --bun vite",
		"dev:op": "NODE_ENV=development bun --bun 1password vite",
		"build": "bun 1password react-router build",
		"start": "NODE_ENV=production bun 1password bun ./build/server/index.js",
		"start:dev": "NODE_ENV=production bun 1password bun ./build/server/index.js",
		"typegen": "react-router typegen",
		"extract:strings": "i18next --config i18next-parser.config.js",
		"validate": "bun run check && bun run typecheck && bun run",
		"check": "biome check .",
		"check:fix": "biome check --fix .",
		"typecheck": "react-router typegen && tsc",
		"format": "biome format ./ --write",
		"lint": "biome lint ./ --write",
		"bump": "bunx npm-check-updates --packageManager bun --interactive --format group",
		"nuke": "rimraf node_modules bun.lock && bun install",
		"icon": "bunx @sly-cli/sly add lucide-icons"
	},
	"dependencies": {
		"@conform-to/react": "^1.5.0",
		"@conform-to/zod": "^1.5.0",
		"@epic-web/cachified": "^5.5.2",
		"@epic-web/client-hints": "^1.3.5",
		"@epic-web/config": "^1.20.1",
		"@epic-web/invariant": "^1.0.0",
		"@forge42/seo-tools": "^1.3.0",
		"@nichtsam/helmet": "^0.3.0",
		"@oslojs/crypto": "^1.0.1",
		"@oslojs/encoding": "^1.1.0",
		"@radix-ui/react-dialog": "^1.1.13",
		"@radix-ui/react-dropdown-menu": "^2.1.14",
		"@radix-ui/react-navigation-menu": "^1.2.12",
		"@radix-ui/react-tooltip": "^1.2.6",
		"@react-router/node": "^7.6.0",
		"@sentry/react-router": "9.17.0",
		"@tailwindcss/vite": "^4.1.6",
		"@tusbar/cache-control": "^1.0.2",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"hono-rate-limiter": "^0.4.2",
		"i18next": "^25.1.2",
		"i18next-browser-languagedetector": "^8.1.0",
		"i18next-http-backend": "^3.0.2",
		"is-ip": "^5.0.1",
		"isbot": "^5.1.28",
		"posthog-js": "^1.240.6",
		"posthog-node": "^4.17.1",
		"react": "19.1.0",
		"react-dom": "19.1.0",
		"react-i18next": "^15.5.1",
		"react-router": "^7.6.0",
		"react-router-devtools": "^5.0.5",
		"react-router-dom": "^7.6.0",
		"react-router-hono-server": "^2.13.0",
		"react-slick": "^0.30.3",
		"remix-hono": "^0.0.18",
		"remix-i18next": "^7.2.1",
		"remix-utils": "^8.7.0",
		"slick-carousel": "^1.8.1",
		"source-map-support": "^0.5.21",
		"spin-delay": "^2.0.1",
		"swiper": "^11.2.6",
		"tailwind-merge": "^3.3.0",
		"tailwindcss": "^4.1.6",
		"tailwindcss-radix": "^4.0.2",
		"tw-animate-css": "^1.2.9",
		"vite-plugin-icons-spritesheet": "^3.0.1",
		"zod": "^3.24.4"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@react-router/dev": "^7.6.0",
		"@sly-cli/sly": "^2.1.1",
		"@types/bun": "^1.2.13",
		"@types/node": "^22.15.17",
		"@types/react": "^19.1.3",
		"@types/react-dom": "^19.1.4",
		"@types/source-map-support": "^0.5.10",
		"i18next-parser": "^9.3.0",
		"typescript": "^5.8.3",
		"vite": "^6.3.5",
		"vite-tsconfig-paths": "^5.1.4"
	},
	"packageManager": "bun@1.2.13",
	"trustedDependencies": [
		"@biomejs/biome",
		"@sentry/cli",
		"@tailwindcss/oxide",
		"core-js",
		"esbuild",
		"unrs-resolver"
	]
}
